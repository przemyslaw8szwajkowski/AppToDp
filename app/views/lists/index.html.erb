<h1>Lists</h1>

<section class = "code-wrapper">
<%= render partial: "task_infinite"%>
<%= render partial: "task_done"%>

<%= @point %>
<%= link_to 'New List', new_list_path, class: "popup btn btn-default" %>
</section>

<script type="text/javascript">
    $('a.popup').click(function() {
  $('<div />').appendTo('body').load($(this).attr('href') + ' form').dialog({dialogClass:'dialog_style1',
    title: $(this).text()
  });
  return false;
});
</script>

	